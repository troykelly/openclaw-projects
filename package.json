    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.20",
    "@types/pg": "^8.16.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cron-parser": "^5.5.0",
    "dompurify": "^3.3.1",
    "fastify": "^5.7.2",
    "highlight.js": "^11.11.1",
    "jose": "^6.1.3",
    "katex": "^0.16.28",
    "lexical": "^0.40.0",
    "lucide-react": "^0.563.0",
    "mermaid": "^11.12.2",
    "mqtt": "^5.15.0",
    "pg": "^8.18.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "^7.13.0",
    "rrule": "^2.8.1",
    "tailwind-merge": "^3.4.0",
    "twilio": "^5.12.0",
    "uuid": "^13.0.0",
    "ws": "^8.19.0",
    "zustand": "^5.0.11"
  }
}
