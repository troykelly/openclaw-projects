{
  "name": "@troykelly/openclaw-projects",
  "version": "0.0.3",
  "description": "OpenClaw memory plugin with projects, todos, and contacts integration",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./register": {
      "types": "./dist/register-openclaw.d.ts",
      "import": "./dist/register-openclaw.js"
    }
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "skills"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run test && pnpm run typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troykelly/openclaw-projects.git",
    "directory": "packages/openclaw-plugin"
  },
  "bugs": {
    "url": "https://github.com/troykelly/openclaw-projects/issues"
  },
  "homepage": "https://github.com/troykelly/openclaw-projects/tree/main/packages/openclaw-plugin#readme",
  "keywords": [
    "openclaw",
    "plugin",
    "memory",
    "projects",
    "todos",
    "contacts"
  ],
  "author": "Troy Kelly",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "openclaw": "*"
  },
  "peerDependenciesMeta": {
    "openclaw": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "openclaw": {
    "extensions": ["dist/register-openclaw.js"]
  }
}
